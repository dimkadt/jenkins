{
  "convertable": false,
  "jps": "https://raw.githubusercontent.com/jelastic-jps/jenkins/master/addons/auto-clustering/auto-cluster-certified.jps",
  "defaultState": true,
  "required": true,
  "compatibleAddons": [
    "jenkins-auto-cluster"
  ],
  "nodeGroupData": {
    "skipNodeEmails": false,
    "scalingMode": "STATEFUL",
    "adminUrl": "",
    "isResetServicePassword": true,
    "isDeploySupport": false,
    "fixedCloudlets": 12,
    "flexibleCloudlets": 16
  },
  "validation": {
    "maxCount": 1
  },
  "recommended": {
    "cloudlets": 16
  },
  "description": "<h3>Jenkins CI/CD Automation Software</h3>Ð¡lustered solution with pre-installed plugins to support building and deployment automation for organizing continuous integration (CI) and continuous delivery (CD) pipelines. <a href='https://jelastic.com/blog/jenkins-cluster-hosting-continuous-integration-delivery/'>Learn more</a> <img width='250' height='250' src='https://raw.githubusercontent.com/jelastic/icons/master/jenkins/jelastic/images/tip-cluster-ps.svg?sanitize=true'>",
  "skipOnEnvInstall": true,
  "extraNodes": {
    "nodeGroup": "slaves",
    "nodeType": "javaengine",
    "engine": "openjdk11",
    "fixedCloudlets": 8,
    "flexibleCloudlets": 16,
    "diskLimit": "100G",
    "displayName": "Workers",
    "count": "${settings.nodes:1}",
    "isClusterSupport": false,
    "isDeploySupport": false,
    "isClusterDependency": true,
    "scalingMode": "STATELESS",
    "applyQuotas": true
  }
}
